CREATE TABLE claim_fact (
  claimkey integer primary key,
  claimID VARCHAR,
  patientkey VARCHAR,
  providerkey VARCHAR,
  servicedatekey VARCHAR,
  procedurekey VARCHAR,
  diagkey VARCHAR,
  claim_amount float,
  claim_status VARCHAR,
  product_type VARCHAR,
  claim_type VARCHAR,
  taxgroup_id VARCHAR,
  bill_type VARCHAR,
  pos VARCHAR
)

Create Table dim_claim (
  claimkey integer primary key,
  claimID VARCHAR, 
  claim_status VARCHAR,
)

Create Table dim_patient (
  patientkey VARCHAR primary key,
  id VARCHAR,
  patient_name VARCHAR,
  dob date,
  address VARCHAR
)

Create Table dim_provider(
  providerkey VARCHAR primary key,
  provider_id VARCHAR,
  provider_name VARCHAR
)


